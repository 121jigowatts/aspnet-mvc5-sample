
@{
    ViewBag.Title = "Log File Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Log File Index</h2>

<table class="table">
    @foreach (var file in ViewBag.Files)
    {
        <tr>
            <td>
                @Html.ActionLink(file as String, "Display", new { name = file })
            </td>
        </tr>
    }
</table>
